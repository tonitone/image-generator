{
    "eslint.runtime": "node",
    "eslint.enable": true,
    "eslint.debug": true,
    "eslint.format.enable": true,
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescriptreact"
    ],
    "eslint.workingDirectories": [
        "./src/"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    }
}